pipeline {
    agent any

    stages {
        stage('Checkout Code from githubcheckout scm
                echo "Checked out code from GitHub') {
            steps {
                git branch: 'main', url: 'https://github.com/Sudarsanan2004/node-app.git'
            }
        }
        stage('Build') {
            steps {
                checkout scm
                echo "Checked out code from GitHub"
            }
        }
        stage('Run index.js') {
            steps {
                echo 'ðŸ§ª Running index.js...'
                sh 'node index.js'
            }
        }
        stage('Run test.js') {
            steps {
                echo ' Running test.js...'
		sh 'node test.js'
            }
        }
	 post {
        success {
            echo "Pipeline completed successfully!"
        }
        failure {
            echo "Pipeline failed!"
        }
    }
    }
}
